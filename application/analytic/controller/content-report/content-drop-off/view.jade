.row
	.col-md-12
		h1: b Drop Off Content
	.col-md-12
		sbd-date-range-select(ng-start-date='param.startDate' ng-end-date='param.endDate')
	.col-md-12
		sbd-total-summary-each-module(
		ng-form-summary='formSummary' ng-data-summary='dataSummary'
		ng-title='"Drop Off Summary"')
	.col-md-12.mr-top-50
		.module-chart
			.box.box-warning
				.box-header
					h3.box-title
						i.fa.fa-area-chart.mr-right-10
						span.mr-5 Drop Off Chart
						i.fa.fa-question-circle(sbd-tooltip ng-value='title')
					.clearfix
				.box-body
					.row
						.col-md-12
							canvas#dropOffChart( width="900" height="500")
	.col-md-12.mr-top-50
		sbd-table-data(ng-form-table='form' ng-data-table='data'
		ng-title='"Detail Drop Off"'
		ng-pagination='pagination'
		ng-action-download='actionDownload'
		ng-sort-by-key='sortByKey')