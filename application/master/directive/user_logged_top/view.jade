		// User Account: style can be found in dropdown.less
		li.dropdown.user.user-menu
			a.dropdown-toggle(href='#', data-toggle='dropdown')
				i.glyphicon.glyphicon-user
				span
					| {{user.name || 'No name'}}
					i.caret
			ul.dropdown-menu
				// User image
				li.user-header.bg-light-blue
					img.img-circle(src='{{user.avatar || "img/avatar1.png"}}', alt='User Image')
					p
						| {{user.email || 'No email'}}
						small Created At: {{ user.createdAt | date }}
				// Menu Body
				li.user-body
					.col-xs-4.text-center
						a(href='#') Followers
					.col-xs-4.text-center
						a(href='#') Sales
					.col-xs-4.text-center
						a(href='#') Friends
				// Menu Footer
				li.user-footer
					.pull-left
						a.btn.btn-default.btn-flat(href='/admin#/account') Profile
					.pull-right
						a.btn.btn-default.btn-flat( ng-click='logoutFE()' ) Sign out